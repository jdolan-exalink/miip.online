{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "env": {
    "VITE_GA_MEASUREMENT_ID": "@ga_measurement_id"
  },
  "routes": [
    {
      "src": "/ads.txt",
      "dest": "/ads.txt"
    },
    {
      "src": "/favicon.svg",
      "dest": "/favicon.svg"
    },
    {
      "src": "^/(?!assets).*",
      "dest": "/index.html"
    }
  ]
}
